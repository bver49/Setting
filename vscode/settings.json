{
    "grunt.autoDetect": "off",
    "jake.autoDetect": "off",
    "npm.autoDetect": "off",
    "gulp.autoDetect": "off",
    "extensions.ignoreRecommendations": true,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Monokai Dark Soda",
    "editor.fontFamily": "FiraCode-Retina, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.scrollBeyondLastLine": false,
    "editor.fontLigatures": true,
    "explorer.confirmDragAndDrop": false,
    "html.autoClosingTags": false,
    "editor.smoothScrolling": true,
    "gitlens.statusBar.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "debug.internalConsoleOptions": "neverOpen",
    "files.exclude": {
        "_ide_helper.php": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/Admin/store/Classes/PHPExcel/**": true,
        "**/cdn": true,
        "**/CVS": true,
        "**/grandPeak": true,
        "**/includes/PHPExcel/**": true,
        "**/node_modules": true,
        "**/themes": true,
        "**/vendor": true,
        "**/webfontkit": true,
        "pls.phpstorm.php": true
    },
    "files.watcherExclude": {
        "_ide_helper.php": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/Admin/store/Classes/PHPExcel/**": true,
        "**/node_modules/**": true,
        "**/vendor/**": true,
        "pls.phpstorm.php": true
    },
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "emmet.excludeLanguages": [
        "markdown",
        "php"
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "php.validate.enable": false,
    "php.suggest.basic": false,
    "python.jediEnabled": false,
    "extensions.autoUpdate": false,
    "gitlens.views.repositories.files.layout": "list",
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/.history/**",
        "**/vendor/**/{Tests,tests}/**"
    ],
    "tabnine.experimentalAutoImports": true,
    "update.mode": "manual",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.php": "default"
    },
    "python.languageServer": "Default",
    "git.ignoreMissingGitWarning": true,
    "yaml.customTags": [
        "!encrypted/pkcs1-oaep scalar",
        "!vault scalar"
    ],
    "yaml.schemaStore.enable": false,
    "workbench.iconTheme": "vscode-icons",
    "bracketPairColorizer.depreciation-notice": false,
    "redhat.telemetry.enabled": true,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.guides.indentation": true,
    "editor.inlineSuggest.enabled": true,
    "git.openRepositoryInParentFolders": "never",
    "editor.scrollbar.verticalScrollbarSize": 12,
    "editor.mouseWheelScrollSensitivity": 0.5,
    "window.zoomLevel": 1,
    "git.enableSmartCommit": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.hideUnchangedRegions.enabled": true
}
